<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gamestoremanagement.controller.EditProductFormController">
    <Label text="Edit Product" />
    <TextField fx:id="nameField" promptText="Name" />
    <TextField fx:id="typeField" promptText="Type" />
    <TextField fx:id="priceField" promptText="Price" />
    <TextField fx:id="quantityField" promptText="Quantity" />
    <TextField fx:id="categoryField" promptText="Category" />
    <TextField fx:id="launchDateField" promptText="Launch Date" />
    <Button text="Save Product" onAction="#handleSaveProduct" />
</VBox>